{
  "name": "ski-analytics-web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "ng test --watch=false",
    "coverage": "ng test --coverage",
    "sendcoverage": "ng test --coverage --maxWorkers=2 && cat ./coverage/lcov.info | codacy-coverage --token $CODACY_TOKEN",
    "test:watch": "ng test --watch",
    "sourcemap": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --source-map --build-optimizer --aot --optimization --vendor-chunk=false --stats-json && npx webpack-bundle-analyzer dist/stats-es2015.json",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci:start-server": "angular-http-server --path ./dist -p 4200",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "ci:cy-run": "CYPRESS_BASE_URL=http://ci-app.mccullough-solutions.ca cypress run",
    "ci:cy-run-local" : "ci:start-server http://localhost:4200  cy:run"
  },
  "private": true,
